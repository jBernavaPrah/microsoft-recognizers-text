{
  "name": "@microsoft/recognizers-text",
  "version": "1.3.0",
  "description": "recognizers-text provides base classes for robust recognition and resolution of text entities.",
  "author": "Microsoft Corp.",
  "license": "MIT",
  "keywords": [
    "nlp",
    "nlp-entity-extraction",
    "entity-extraction",
    "parser-library"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/Recognizers-Text"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/Recognizers-Text/issues"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "xregexp": "^5.1.1"
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.es5.js",
  "types": "dist/types/index.d.ts",
  "browser": "dist/index.browser.js",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c && tsc -b tsconfig.types.json",
    "watch": "concurrently --kill-others \"rollup -c --watch\" \"tsc -b tsconfig.types.json --watch\" ",
    "prettier": "prettier -w -l src test package.json",
    "eslint": "eslint src test --fix --ext .ts",
    "rimraf": "rimraf dist",
    "test": "jest --runInBand"
  }
}
