{
  "name": "@microsoft/recognizers-text-number",
  "version": "1.3.0",
  "description": "recognizers-text-number provides robust recognition and resolution of numbers expressed in multiple languages.",
  "author": "Microsoft Corp.",
  "license": "MIT",
  "keywords": [
    "nlp",
    "nlp-entity-extraction",
    "entity-extraction",
    "numex",
    "parser-library"
  ],
  "main": "dist/index.umd.js",
  "module": "dist/index.es5.js",
  "types": "dist/types/index.d.ts",
  "browser": "dist/index.browser.js",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/Recognizers-Text"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/Recognizers-Text/issues"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c && tsc -b tsconfig.types.json",
    "watch": "concurrently --kill-others \"rollup -c --watch\" \"tsc -b tsconfig.types.json --watch\" ",
    "prettier": "prettier -w -l src test package.json",
    "eslint": "eslint src test --fix --ext .ts",
    "rimraf": "rimraf dist",
    "test": "jest --runInBand",
    "build-resources": "ts-node --project ../resource-generator/tsconfig.json ../resource-generator/ ../recognizers-number/resource-definitions.json"
  },
  "dependencies": {
    "@microsoft/recognizers-text": "~1.3.0",
    "bignumber.js": "^9.1.1",
    "lodash.escaperegexp": "^4.1.2",
    "lodash.sortby": "^4.7.0",
    "lodash.trimend": "^4.5.1"
  }
}
