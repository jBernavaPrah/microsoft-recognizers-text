{
	"name": "recognizers",
	"private": true,
	"version": "1.3.0",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/Recognizers-Text"
	},
	"bugs": {
		"url": "https://github.com/Microsoft/Recognizers-Text/issues"
	},
	"scripts": {
		"lint": "eslint **/*.ts --ignore-pattern node_modules/",
		"lint-fix": "eslint **/*.ts --ignore-pattern node_modules/ --fix",
		"browserify": "browserify -d -o ./test/browser-specs/index.js package.json ./test/runner-browser.js",
		"browser-test": "npm run browserify && node ./test/browser-specs/server.js",
		"setup": "npm run bootstrap && npm run build",
		"bootstrap": "lerna bootstrap --hoist --no-ci --nohoist=@babel/* --nohoist=babel-*",
		"clean": "lerna clean",
		"build": "lerna exec npm run build --ignore @microsoft/recognizers-text-date-time",
		"prettier": "lerna exec npm run prettier ",
		"eslint": "lerna exec npm run eslint ",
		"rimraf": "lerna exec npm run rimraf ",
		"test": "lerna exec npm run test ",
		"version": "node scripts/update-peer-dependencies.js",
		"preparePublish": "npm run bootstrap && npm run prettier && npm run eslint && npm run build && npm run test",
		"updateVersions:minor": "lerna version minor --yes --no-push --no-git-tag-version",
		"updateVersions:patch": "lerna version patch --yes --no-push --no-git-tag-version",
		"publishPackages": "lerna publish from-package --yes --no-verify-access"
	},
	"browserify-shim": {
		"@microsoft/recognizers-text": "global:microsoftRecognizersTextBase",
		"@microsoft/recognizers-text-choice": "global:microsoftRecognizersTextChoice",
		"@microsoft/recognizers-text-number": "global:microsoftRecognizersTextNumber",
		"@microsoft/recognizers-text-number-with-unit": "global:microsoftRecognizersTextNumberWithUnit",
		"@microsoft/recognizers-text-sequence": "global:microsoftRecognizersSequence",
		"@microsoft/recognizers-text-date-time": "global:microsoftRecognizersTextDateTime"
	},
	"browserify": {
		"transform": [
			"browserify-shim",
			"babelify"
		]
	},
	"ava": {
		"files": [
			"./test/index-number.js",
			"./test/index-number-with-unit.js",
			"./test/index-datetime.js",
			"./test/index-choice.js",
			"./test/index-sequence.js"
		]
	},
	"devDependencies": {
		"@rollup/plugin-alias": "^5.0.0",
		"@rollup/plugin-babel": "^6.0.3",
		"@rollup/plugin-commonjs": "^25.0.0",
		"@rollup/plugin-node-resolve": "^15.0.2",
		"@rollup/plugin-typescript": "^11.1.1",
		"@types/jest": "^29.5.1",
		"@types/js-yaml": "^4.0.1",
		"@types/lodash.escaperegexp": "^4.1.7",
		"@types/lodash.isequal": "^4.5.6",
		"@types/lodash.last": "^3.0.7",
		"@types/lodash.max": "^4.0.7",
		"@types/lodash.sortby": "^4.7.7",
		"@types/lodash.tonumber": "^4.0.7",
		"@types/lodash.trimend": "^4.5.7",
		"@types/node": "^16.3.0",
		"@types/npm": "^7.0.7",
		"@typescript-eslint/eslint-plugin": "^4.28.5",
		"@typescript-eslint/parser": "^4.28.5",
		"acorn": "^8.6.0",
		"ava": "^3.15.0",
		"ava-spec": "^1.1.1",
		"babel-eslint": "^10.1.0",
		"babel-plugin-external-helpers": "^6.22.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-stage-0": "^6.24.1",
		"babelify": "^10.0.0",
		"braces": "^3.0.2",
		"browserify": "^17.0.0",
		"browserify-shim": "^3.8.14",
		"cached-path-relative": "^1.0.2",
		"colors": "^1.4.0",
		"concurrently": "^8.0.1",
		"dot-prop": "^8.0.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-only-warn": "^1.1.0",
		"eslint-utils": "^2.1.0",
		"express": "^4.17.1",
		"handlebars": "^4.7.7",
		"jest": "^29.5.0",
		"js-yaml": "^4.1.0",
		"lerna": "^6.6.2",
		"lodash": "^4.17.21",
		"lodash.camelcase": "^4.3.0",
		"lodash.keys": "^4.2.0",
		"lodash.merge": "^4.6.2",
		"lodash.template": "^4.5.0",
		"mem": "^9.0.2",
		"minimist": "^1.2.8",
		"mixin-deep": "^2.0.1",
		"path-parse": "^1.0.7",
		"prettier": "^2.4.1",
		"rimraf": "^3.0.2",
		"rollup": "^2.56.2",
		"ts-jest": "^29.0.4",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4"
	},
	"workspaces": [
		"packages/recognizers-*"
	],
	"engines": {
		"node": ">=16"
	}
}
